{
  "configurations": [
    {
      "name": "windows-gcc-x86",
      "defines": [
        "_DEBUG",
        "UNICODE"
      ],
      "compilerPath": "C:/MinGW/bin/gcc.exe",
      "intelliSenseMode": "windows-gcc-x86",
      "browse": {
        "path": [
          "${workspaceFolder}"
        ],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": ""
      },
      "includePath": [
        "${workspaceFolder}/**"
      ],
      "cStandard": "${default}",
      "cppStandard": "${default}"
    }
  ],
  "version": 4
}